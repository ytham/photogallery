    <hr>
  <% if current_user %>
    <% if !current_user.likes.find_by_photo_id(@photo.id).blank? %>
      <%= link_to 'Unlike', { controller: 'likes', action: 'destroy', id: @photo.id }, method: 'delete' %>
    <% else %>
      <%= link_to 'Like', { controller: 'likes', action: 'create', id: @photo.id }, method: 'post' %>
    <% end %>
  <% end %>
  <% if !@user_likes.blank? %>
  <br />
    <% @user_likes.each do |u| %>
      <%= link_to u.name, u %>  
    <% end %>
  <% end %>